{
   "name": "@eldritch-engine/builder-core-mangler",
   "version": "0.1.0-pre.alpha.1",
   "description": "core build-time logic for symbol mangling and constant folding optimizations",
   "license": "MPL-2.0",
   "private": false,
   "files": [
      "dist/",
      "src/"
   ],
   "exports": {
      "./package.json": "./package.json",
      ".": {
         "source": "./src/index.ts",
         "types": "./dist/index.ts",
         "import": "./src/index.ts",
         "require": "./src/index.ts"
      },
      "./*": {
         "source": "./src/*.ts",
         "types": "./dist/*.ts",
         "import": "./src/*.ts",
         "require": "./src/*.ts"
      }
   },
   "dependencies": {
      "@eldritch-engine/type-utils": "workspace:*",
      "@swc/core": "^1.13.20"
   },
   "devDependencies": {
      "@eldritch-engine/tsconfig": "workspace:*"
   }
}